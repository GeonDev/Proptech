<div class="card-body pt-0">
    <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
            <thead>
            <tr>
                {{#pages.columnTitles}}
                    <th>{{title}}</th>
                {{/pages.columnTitles}}
            </tr>
            </thead>

            <tbody>
                {{#pages.contents}}
                    <tr>
                        <td>{{name}}</td>
                        <td>{{email}}</td>
                        <td>{{phoneNumber}}</td>
                        <td>{{provider}}</td>
                        <td>{{role}}</td>
                        <td>{{state}}</td>
                        <td>{{registerDate}}</td>
                        <td>{{retiredDate}}</td>
                    </tr>
                {{/pages.contents}}
            </tbody>
        </table>
    </div>
</div>